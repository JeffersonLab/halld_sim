c	77	a2(1320)
c	80	Lambda_c +
c	90	J/psi
c	91	Baryonium
c	92	Diproton
c	93	Lambda anti-Lambda state
c	94	Lambda anti-Lambda state
c	95	proton anti-Lambda state
c	98	"Fat Meson"
c	99	"Fat Baryon"
c	100	"Box-like" mass distribution
c
